{
  "editor.tabSize": 4,
  "editor.insertSpaces": true,
  "files.eol": "\n",
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/node_modules/**": true
  },
  "search.exclude": {
    ".tmp": true,
    "typings": true
  },
  "json.schemas": [
    {
      "fileMatch": [
        "/pbiviz.json"
      ],
      "url": "./node_modules/powerbi-visuals-tools/templates/visuals/.api/v1.7.0/schema.pbiviz.json"
    },
    {
      "fileMatch": [
        "/capabilities.json"
      ],
      "url": "./node_modules/powerbi-visuals-tools/templates/visuals/.api/v1.7.0/schema.capabilities.json"
    },
    {
      "fileMatch": [
        "/dependencies.json"
      ],
      "url": "./node_modules/powerbi-visuals-tools/templates/visuals/.api/v1.6.0/schema.dependencies.json"
    }
  ]
}
