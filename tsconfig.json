{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "jsx": "react",
    "jsxFactory": "createElement",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "declaration": true,
    "noImplicitAny": false,
    "noUnusedLocals": true,
    "sourceMap": true,
    "suppressImplicitAnyIndexErrors": true,
    "outDir": "lib",
    "lib": [
      "es2017",
      "dom"
    ]
  }
}
